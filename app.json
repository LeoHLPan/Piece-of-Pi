{
  "expo": {
    "name": "Piece of Pi",
    "slug": "piece-of-pi",
    "description": "Want to memorize mathematical constants like pi? This app breaks them up into smaller groups so you can memorize the digits bit by bit. Remember, it's always better to take your time!",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#404040"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.interger.pieceofpi"
    },
    "android": {
      "package": "com.interger.pieceofpi",
      "versionCode": 2,
      "permissions": []
    },
    "androidStatusBar": {
      "backgroundColor": "#000000"
    }
  }
}
